  <h2>Lista de productos</h2>
  {{#each productsList}}

    <body>
                <div class="CardItem">
            <div>
                <h2 class="ItemHeader">
                   {{this.title}}
                </h2>
            </div>
            <div>
                <img class="ItemImg" src="{{this.thumbnail}}" alt="">
            </div>
            <div>
                <p className="Info">
                {{description}}</p>

            </div>
            <div>
                <p className="Info">
                Precio: {{this.price}}</p>
            </div>
        </div>
    </body>



{{/each}}